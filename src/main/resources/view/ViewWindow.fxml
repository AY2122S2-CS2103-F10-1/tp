<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.stage.Stage?>

<?import javafx.scene.layout.VBox?>
<fx:root resizable="false" title="Help" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11"
         xmlns:fx="http://javafx.com/fxml/1" minWidth="300">
    <icons>
        <Image url="@/images/help_icon.png"/>
    </icons>
    <scene>
        <Scene>
            <stylesheets>
                <URL value="@ViewWindow.css"/>
            </stylesheets>

            <VBox fx:id="viewContainer">
                <children>
                    <Label fx:id="studentName">
                        <padding>
                            <Insets bottom="15.0"/>
                        </padding>
                    </Label>
                    <Label fx:id="labDetails">
                        <Insets top="10.0"/>
                    </Label>
                </children>
                <opaqueInsets>
                    <Insets bottom="10.0" left="5.0" right="10.0" top="5.0"/>
                </opaqueInsets>
                <padding>
                    <Insets bottom="10.0" left="5.0" right="10.0" top="5.0"/>
                </padding>
            </VBox>
        </Scene>
    </scene>
</fx:root>
